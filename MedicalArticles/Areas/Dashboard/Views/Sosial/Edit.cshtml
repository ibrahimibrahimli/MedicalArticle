@model Sosial

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="WhatsappUrl" class="control-label">İstifadəçi adı</label>
                <input asp-for="WhatsappUrl" class="form-control" />
                <span asp-validation-for="WhatsappUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FacebookUrl" class="control-label">E-Poçt</label>
                <input asp-for="FacebookUrl" class="form-control" id="answer"></input>
                <span asp-validation-for="FacebookUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InstagramUrl" class="control-label">Telefon</label>
                <input asp-for="InstagramUrl" class="form-control" id="answer"></input>
                <span asp-validation-for="InstagramUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Telegram" class="control-label">Mesaj</label>
                <input asp-for="Telegram" class="form-control" id="answer"></input>
                <span asp-validation-for="Telegram" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="TwitterUrl" class="control-label">Mesaj</label>
                <input asp-for="TwitterUrl" class="form-control" id="answer"></input>
                <span asp-validation-for="TwitterUrl" class="text-danger"></span>
            </div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <input type="submit" value="@DefaultConstantValues.ADD_MESSAGE" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@DefaultConstantValues.RETURN_TO_BACK</a>
</div>
